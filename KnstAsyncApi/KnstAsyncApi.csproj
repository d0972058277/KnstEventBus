<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageVersion>1.0.2-pre</PackageVersion>
    <Authors>PorriDG3</Authors>
    <Owners>PorriDG3</Owners>
    <PackageProjectUrl>https://github.com/d0972058277/KnstEventBus</PackageProjectUrl>
    <Summary>AsyncApi code-first document generator</Summary>
    <Title>KnstAsyncApi</Title>
